{
  "name": "newspiper",
  "version": "1.0.0",
  "description": "Newspiper is a modern WordPress theme, tailored using latest coding standards and following best development practices.",
  "scripts": {
    "build": "wp-scripts build",
    "postbuild": "rimraf build/**/*.map",
    "start": "wp-scripts start",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "format:css": "npm run lint:css -- --fix",
    "format:js": "npm run lint:js -- --fix"
  },
  "devDependencies": {
    "@wordpress/dependency-extraction-webpack-plugin": "^4.6.0",
    "@wordpress/eslint-plugin": "^14.8.0",
    "@wordpress/scripts": "^24.6.0",
    "mini-css-extract-plugin": "^2.7.1",
    "rtlcss": "^4.0",
    "webpack-fix-style-only-entries": "^0.6.1"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "author": "Atanas Yonkov",
  "license": "GNU GPL"
}
